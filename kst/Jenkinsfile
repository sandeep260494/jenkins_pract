pipeline {
	agent {
        label 'KST-AGENT'
    }
	stages {
		stage('git clone'){
			steps{
				echo "this is a GIT CLONE STAGE"
			}
		}
        stage('TEST') {
            steps {
                script {
                    echo "THIS IS A TEST STAGE"
                }
            }
        }
        stage('BUILD'){
            steps {
                script {
                    sh """
                        uname -a
                        java --version
                        // git --version
                    """
                }
            }
        }
	}
}